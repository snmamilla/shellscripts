pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                sh 'git --version'
            }
        }
    
        stage('test') {
            steps {
                sh 'python --version'
            }
        }
         stage ('Check logs') {
                    steps {
                       echo "All are working fine."
                    }
                }
    }    
}
